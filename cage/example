USED=no
BOOT=yes
TARG=/glftpd
TYPE=tmpfs
SIZE=400
CMOD=0700
MOPT="-o noatime"
STOR=/root/.zoo
KEEP=3
TOPT="--exclude=/glftpd/site/* --exclude=/glftpd/lost+found"
MNTC="/dev/vdb1:/glftpd/site /dev/vdb2:/glftpd/site/_arc/_kids /dev/vdb3:/glftpd/site/_arc/series"
SRVC="systemd:glftpd.socket samba nfs-kernel-server"
